<?xml version="1.0" encoding="utf-8" ?>

<configuration>

  <system.webServer>

    <security>
      <requestFiltering removeServerHeader="true" />
    </security>

    <httpProtocol>
      <customHeaders>
        <clear />
        <remove name="X-Powered-By" />
        <remove name="Server" />
      </customHeaders>
    </httpProtocol>

  </system.webServer>

  <system.web>
    <httpRuntime enableVersionHeader="false"/>
  </system.web>

</configuration>